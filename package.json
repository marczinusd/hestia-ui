{
  "name": "hestia-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --maxWorkers=1",
    "test:coverage": "jest --coverage --maxWorkers=1",
    "test:watch": "jest --watchAll",
    "lint": "ng lint && eslint . --ext .ts,.json --ignore-pattern node_modules/",
    "lint:fix": "tslint --fix -c tslint.json --project tsconfig.json && eslint . --ext .ts,.json --ignore-pattern node_modules/ --fix",
    "e2e": "cypress open",
    "e2e:start": "start-server-and-test start http-get://localhost:4200 e2e",
    "e2e:cover": "cypress open --env coverage=true",
    "e2e:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "e2e:ci:cover": "start-server-and-test start http-get://localhost:4200 cy:run:cover",
    "e2e:ci:quiet": "start-server-and-test start http-get://localhost:4200 cy:run:quiet",
    "cy:run": "cypress run",
    "cy:run:quiet": "cypress run -q --reporter=list",
    "cy:run:cover": "cypress run --env coverage=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.3",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.3",
    "@angular/compiler": "^10.2.3",
    "@angular/core": "^10.2.3",
    "@angular/forms": "^10.2.3",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.3",
    "@angular/platform-browser-dynamic": "^10.2.3",
    "@angular/router": "^10.2.3",
    "@datorama/akita": "^5.2.5",
    "ag-grid-angular": "^24.1.0",
    "ag-grid-community": "^24.1.0",
    "chart.js": "^2.9.4",
    "chartjs": "^0.3.24",
    "chartjs-plugin-annotation": "^0.5.7",
    "lodash": "^4.17.20",
    "ng2-charts": "^2.4.2",
    "ngx-monaco-editor": "^9.0.0",
    "rxjs": "~6.6.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.0.8",
    "@angular/compiler-cli": "^10.2.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^10.0.0",
    "@cypress/code-coverage": "^3.8.3",
    "@cypress/webpack-preprocessor": "^5.4.11",
    "@datorama/akita-ng-router-store": "^5.1.8",
    "@datorama/akita-ngdevtools": "^5.0.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@ngneat/spectator": "^6.1.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "babel-plugin-istanbul": "^6.0.0",
    "codelyzer": "^6.0.1",
    "cypress": "^5.6.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "lint-staged": "^10.5.1",
    "ng-mocks": "^11.0.0",
    "ngx-build-plus": "^10.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.11.5",
    "ts-loader": "8.0.2",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.3",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
