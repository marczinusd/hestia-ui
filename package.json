{
  "name": "hestia-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest --maxWorkers=1",
    "test:coverage": "jest --coverage --maxWorkers=1",
    "test:watch": "jest --watchAll",
    "lint": "ng lint && eslint . --ext .ts,.json --ignore-pattern node_modules/",
    "lint:fix": "tslint --fix -c tslint.json --project tsconfig.json && eslint . --ext .ts,.json --ignore-pattern node_modules/ --fix",
    "e2e": "cypress open",
    "e2e:cover": "cypress open --env coverage=true",
    "e2e:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "e2e:ci:cover": "start-server-and-test start http-get://localhost:4200 cy:run:cover",
    "cy:run": "cypress run",
    "cy:run:cover": "cypress run --env coverage=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.9",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "^10.0.9",
    "@angular/compiler": "^10.0.9",
    "@angular/core": "^10.0.9",
    "@angular/forms": "^10.0.9",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.9",
    "@angular/platform-browser-dynamic": "^10.0.9",
    "@angular/router": "^10.0.9",
    "ag-grid-angular": "^23.2.1",
    "ag-grid-community": "^23.2.1",
    "ngx-monaco-editor": "^9.0.0",
    "rxjs": "~6.6.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.5",
    "@angular/cli": "~10.0.5",
    "@angular/compiler-cli": "^10.0.9",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@cypress/code-coverage": "^3.8.1",
    "@cypress/webpack-preprocessor": "5.4.6",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@ngneat/spectator": "^5.13.4",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "babel-plugin-istanbul": "^6.0.0",
    "codelyzer": "^6.0.0",
    "cypress": "4.12.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.4.0",
    "jest-preset-angular": "^8.2.1",
    "lint-staged": "^10.4.0",
    "ng-mocks": "^10.1.2",
    "ngx-build-plus": "^10.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "source-map-support": "^0.5.19",
    "start-server-and-test": "^1.11.3",
    "ts-loader": "8.0.2",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
